{
    "name": "sevens/jsondb",
    "description": "Simple Php Test JSON Database; useful for development mode testing",
    "type": "library",
    "version": "v0.1.0",
    "license": "MIT",
    "minimum-stability": "stable",
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Seven\\JsonDB\\": "src/",
            "Seven\\JsonDB\\Traits\\": "src/traits/",
            "Seven\\JsonDB\\DB\\": "src/db/",
            "Seven\\JsonDB\\Utils\\": "src/utils/"
        },
        "files": []
    },
    "require": {
        "php": ">=8.0.0",
        "sevens/vars": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4.2"
    },
    "scripts": {
        "post-install-cmd": [
            "@php -r \"mkdir('/vendor/sevens/jsondb/db') && file_put_contents('<?php return[];', '/vendor/sevens/jsondb/db/schema.php');\""
        ]
    },
    "authors": [
        {
            "name": "Elisha Temiloluwa a.k.a TemmyScope",
            "email": "temmyscope@protonmail.com",
            "role": "Developer"
        }
    ],
    "keywords": [
        "db", "json", "test", "database", "engine"
    ]
}
